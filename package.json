{
  "name": "iplan-library-sample",
  "version": "1.0.0",
  "description": "Made with create-react-library",
  "repository": "IPlan/iplan-library-sample",
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.js",
  "scripts": {
    "generate-types": "npx -p typescript tsc",
    "build": "npm run generate-types && microbundle-crl --no-compress --format modern,cjs",
    "start": "npm run generate-types && microbundle-crl watch --no-compress --format modern,cjs"
  },
  "peerDependencies": {
    "react": "^16.0.0"
  },
  "devDependencies": {
    "microbundle-crl": "^0.13.11",
    "npm-dts": "^1.3.12",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-scripts": "^3.4.4",
    "typescript": "^4.7.4"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3",
    "@mui/material": "^5.8.7",
    "react-hook-form": "^7.33.1"
  }
}
